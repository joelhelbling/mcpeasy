{
  "mcpServers": {
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {}
    },
    "github": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "ghcr.io/github/github-mcp-server"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
      }
    },
    "slack": {
      "type": "stdio",
      "command": "./bin/mcpz",
      "args": [
        "slack",
        "mcp"
      ],
      "env": {}
    },
    "gdrive": {
      "type": "stdio",
      "command": "./bin/mcpz",
      "args": [
        "gdrive",
        "mcp"
      ],
      "env": {}
    },
    "gcal": {
      "type": "stdio",
      "command": "./bin/mcpz",
      "args": [
        "gcal",
        "mcp"
      ],
      "env": {}
    },
    "gmeet": {
      "type": "stdio",
      "command": "./bin/mcpz",
      "args": [
        "gmeet",
        "mcp"
      ],
      "env": {}
    }
  }
}
